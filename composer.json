{
    "name": "devront/selling-partner-api",
    "version": "5.3.0",
    "description": "PHP client for Amazon's Selling Partner API",
    "keywords": [
        "api",
        "selling-partner-api",
        "amazon",
        "ecommerce",
        "sp-api",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Jesse Evers",
            "homepage": "https://jesseevers.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4",
        "phpoffice/phpspreadsheet": "^1.19",
        "aws/aws-sdk-php": "^3.185",
        "illuminate/http": "^9.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": { "SellingPartnerApi\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SellingPartnerApi\\Tests\\" : "test/" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
